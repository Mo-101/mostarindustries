VITE_NEON_JWKS_URL="your-neon-jwks-url"

# Authentication Configuration
VITE_NEXT_PUBLIC_NEON_AUTH_URL="your-neon-auth-url"

# PG Admin Details
# ===== Database (shared) =====
VITE_PG_HOST=your-pg-host
VITE_PG_PORT=your-pg-port
VITE_PG_DATABASE=your-pg-database
VITE_PG_USER=your-pg-user
VITE_PG_PASSWORD=your-pg-password
VITE_DATABASE_URL=postgres://your-pg-user:your-pg-password@your-pg-host:your-pg-port/your-pg-database

# If you decide to run the bundled DB container (commented in compose), it will read these:
VITE_POSTGRES_DB=${VITE_PG_DATABASE}
VITE_POSTGRES_USER=${VITE_PG_USER}
VITE_POSTGRES_PASSWORD=${VITE_PG_PASSWORD}

# ===== App DB user created by SQL (grid_setup.sql) =====
VITE_GRID_APP_USER=your-grid-app-user
VITE_GRID_APP_PASS=your-grid-app-pass   # change in prod

# ===== Service ports =====
VITE_TRINITY_PORT=your-trinity-port
VITE_LOOKUP_PORT=your-lookup-port
VITE_TILESERV_PORT=your-tileserv-port
VITE_NGINX_PORT=your-nginx-port
VITE_FLASK_RUN_PORT_TRINITY=${VITE_TRINITY_PORT}
VITE_FLASK_RUN_PORT_LOOKUP=${VITE_LOOKUP_PORT}

# ===== ThroneLock RBAC (header: X-Grid-Key) =====
# Keep the JSON in single quotes to preserve it.
VITE_GRID_KEYS_JSON='{"architect":"your-architect-key","executor":"your-executor-key","guardian":"your-guardian-key"}'
# Smoke test default key (can override)
VITE_HEADER_KEY=your-header-key

# ===== Truth Engine / Codex paths =====
VITE_RULEPACK_PATH=your-rulepack-path
VITE_SOULPRINT_PATH=your-soulprint-path
VITE_WOO_ID_PATH=your-woo-id-path

# ===== Optional Flask secret (sessions) =====
VITE_SECRET_KEY=your-secret-key

# ===== Misc (optional) =====
VITE_APP_ENV=your-app-env

