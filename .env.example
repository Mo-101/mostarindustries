VITE_DATABASE_URL=your-database-url

# Local Development
VITE_NODE_ENV=development
VITE_LOCAL_DATABASE_URL=your-local-database-url

# API Keys
VITE_NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your-google-maps-api-key"
VITE_NEXT_GEMINI_API_KEY="your-gemini-api-key"

